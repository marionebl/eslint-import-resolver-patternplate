{
  "name": "eslint-import-resolver-patternplate",
  "version": "1.0.0",
  "description": "eslint-import integration fro patternplate",
  "main": "index.js",
  "scripts": {
    "start": "npm run test -- --watch",
    "lint": "xo",
    "pretest": "npm run lint && npm run deps",
    "test": "ava",
    "deps": "dependency-check . --missing && dependency-check . --extra --no-dev",
    "update": "tipi update"
  },
  "tipi": {
    "version": "3.1.0",
    "template": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marionebl/eslint-import-resolver-patternplate.git"
  },
  "keywords": [],
  "author": "marionebl <hello@herebecode.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marionebl/eslint-import-resolver-patternplate/issues"
  },
  "homepage": "https://github.com/marionebl/eslint-import-resolver-patternplate#readme",
  "devDependencies": {
    "ava": "0.18.2",
    "dependency-check": "^2.8.0",
    "load-json-file": "2.0.0",
    "tipi-cli": "3.1.0",
    "xo": "^0.17.1"
  },
  "dependencies": {
    "path-exists": "3.0.0",
    "resolve": "1.3.2",
    "resolve-from": "2.0.0"
  }
}
